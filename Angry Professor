#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t,n,k,a[1000];
    cin>>t;
    for(int i=1;i<=t;i++)
        {
        cin>>n;
        cin>>k;
        for(int j=0;j<n;j++)
            {
            cin>>a[j];
        }
        for(int r=0;r<n;r++)
            {
            if(a[r]<=0)
                {
                k--;
            }
        }
        if(k<=0)
            {
            cout<<"NO"<<endl;
        }
        else
            {
            cout<<"YES"<<endl;
        }
        
    }
    return 0;
}
