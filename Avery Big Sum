#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
        int n,arr[10];
    long int sum=0;
    cin>>n;
    for(int i=0;i<n;i++)
        {
        cin>>arr[i];
    }
    for(int j=0;j<n;j++)
        {
        sum=sum+arr[j];
    }
    cout<<sum;
return 0;
}
